{
  "name": "rematch-storage",
  "version": "0.1.0",
  "scripts": {
    "clean": "rm -rf dist/*",
    "lint": "eslint --fix src/",
    "pre-commit": "lint-staged",
    "test": "cross-env BABEL_ENV=test jest --colors --config .jest.js --no-cache",
    "es": "c3-tools compile",
    "lib": "c3-tools compile -t commonjs",
    "simple": "c3-version changelog",
    "cz": "git add . && git cz",
    "major": "npm version major -m 'Upgrade version to %s '",
    "minor": "npm version minor -m 'Upgrade version to %s '",
    "patch": "npm version patch -m 'Upgrade version to %s'",
    "x": "npm run changelog && npm run cz && npm run major",
    "y": "npm run changelog && npm run cz && npm run minor",
    "z": "npm run changelog && npm run cz && npm run patch"
  },
  "main": "src/index.js",
  "sideEffects": false,
  "license": "ISC",
  "files": [
    "src",
    "lib",
    "es"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "lint-staged": {
    "src/**/*.js": [
      "cross-env BABEL_ENV=test jest --findRelatedTests --config .jest.js --no-cache",
      "git add"
    ]
  },
  "eslintIgnore": [
    "dist"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "c3-tools": "^2.0.1-alpha.11",
    "commitizen": "^3.0.5",
    "conventional-changelog-cli": "^2.0.11",
    "cross-env": "^5.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.5",
    "eslint-config-ytian": "^2.1.1",
    "husky": "^1.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-test-renderer": "^16.6.3"
  }
}
